  "verses": [
    {
      "number": 1,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "إِذَا ٱلشَّمۡسُ كُوِّرَتۡ"
    },
    {
      "number": 2,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتۡ"
    },
    {
      "number": 3,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَإِذَا ٱلۡجِبَالُ سُيِّرَتۡ"
    },
    {
      "number": 4,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَإِذَا ٱلۡعِشَارُ عُطِّلَتۡ"
    },
    {
      "number": 5,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَإِذَا ٱلۡوُحُوشُ حُشِرَتۡ"
    },
    {
      "number": 6,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَإِذَا ٱلۡبِحَارُ سُجِّرَتۡ"
    },
    {
      "number": 7,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتۡ"
    },
    {
      "number": 8,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَإِذَا ٱلۡمَوۡءُۥدَةُ سُئِلَتۡ"
    },
    {
      "number": 9,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "بِأَيِّ ذَنۢبٖ قُتِلَتۡ"
    },
    {
      "number": 10,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَإِذَا ٱلصُّحُفُ نُشِرَتۡ"
    },
    {
      "number": 11,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَإِذَا ٱلسَّمَآءُ كُشِطَتۡ"
    },
    {
      "number": 12,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَإِذَا ٱلۡجَحِيمُ سُعِّرَتۡ"
    },
    {
      "number": 13,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَإِذَا ٱلۡجَنَّةُ أُزۡلِفَتۡ"
    },
    {
      "number": 14,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "عَلِمَتۡ نَفۡسٞ مَّآ أَحۡضَرَتۡ"
    },
    {
      "number": 15,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "فَلَآ أُقۡسِمُ بِٱلۡخُنَّسِ"
    },
    {
      "number": 16,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "ٱلۡجَوَارِ ٱلۡكُنَّسِ"
    },
    {
      "number": 17,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَٱلَّيۡلِ إِذَا عَسۡعَسَ"
    },
    {
      "number": 18,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَٱلصُّبۡحِ إِذَا تَنَفَّسَ"
    },
    {
      "number": 19,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ"
    },
    {
      "number": 20,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "ذِي قُوَّةٍ عِندَ ذِي ٱلۡعَرۡشِ مَكِينٖ"
    },
    {
      "number": 21,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "مُّطَاعٖ ثَمَّ أَمِينٖ"
    },
    {
      "number": 22,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَمَا صَاحِبُكُم بِمَجۡنُونٖ"
    },
    {
      "number": 23,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَلَقَدۡ رَءَاهُ بِٱلۡأُفُقِ ٱلۡمُبِينِ"
    },
    {
      "number": 24,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَمَا هُوَ عَلَى ٱلۡغَيۡبِ بِضَنِينٖ"
    },
    {
      "number": 25,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَمَا هُوَ بِقَوۡلِ شَيۡطَٰنٖ رَّجِيمٖ"
    },
    {
      "number": 26,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "فَأَيۡنَ تَذۡهَبُونَ"
    },
    {
      "number": 27,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "إِنۡ هُوَ إِلَّا ذِكۡرٞ لِّلۡعَٰلَمِينَ"
    },
    {
      "number": 28,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "لِمَن شَآءَ مِنكُمۡ أَن يَسۡتَقِيمَ"
    },
    {
      "number": 29,
      "juz": 30,
      "page": 586,
      "sajda": false,
      "ar": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلۡعَٰلَمِينَ"
    }
  ]
}