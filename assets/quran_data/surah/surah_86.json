  "verses": [
    {
      "number": 1,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "وَٱلسَّمَآءِ وَٱلطَّارِقِ"
    },
    {
      "number": 2,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "وَمَآ أَدۡرَىٰكَ مَا ٱلطَّارِقُ"
    },
    {
      "number": 3,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "ٱلنَّجۡمُ ٱلثَّاقِبُ"
    },
    {
      "number": 4,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "إِن كُلُّ نَفۡسٖ لَّمَّا عَلَيۡهَا حَافِظٞ"
    },
    {
      "number": 5,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ مِمَّ خُلِقَ"
    },
    {
      "number": 6,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "خُلِقَ مِن مَّآءٖ دَافِقٖ"
    },
    {
      "number": 7,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "يَخۡرُجُ مِنۢ بَيۡنِ ٱلصُّلۡبِ وَٱلتَّرَآئِبِ"
    },
    {
      "number": 8,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرٞ"
    },
    {
      "number": 9,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "يَوۡمَ تُبۡلَى ٱلسَّرَآئِرُ"
    },
    {
      "number": 10,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "فَمَا لَهُۥ مِن قُوَّةٖ وَلَا نَاصِرٖ"
    },
    {
      "number": 11,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجۡعِ"
    },
    {
      "number": 12,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ"
    },
    {
      "number": 13,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "إِنَّهُۥ لَقَوۡلٞ فَصۡلٞ"
    },
    {
      "number": 14,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "وَمَا هُوَ بِٱلۡهَزۡلِ"
    },
    {
      "number": 15,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "إِنَّهُمۡ يَكِيدُونَ كَيۡدٗا"
    },
    {
      "number": 16,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "وَأَكِيدُ كَيۡدٗا"
    },
    {
      "number": 17,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "ar": "فَمَهِّلِ ٱلۡكَٰفِرِينَ أَمۡهِلۡهُمۡ رُوَيۡدَۢا"
    }
  ]
}