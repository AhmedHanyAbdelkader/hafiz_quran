{
  "number": 80,
  "name": {
    "ar": "عبس"
  },
  "revelation_place": {
    "ar": "مكية"
  },
  "verses_count": 42,
  "words_count": 133,
  "letters_count": 538,
  "verses": [
    {
      "number": 1,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "عَبَسَ وَتَوَلَّىٰٓ"
    },
    {
      "number": 2,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "أَن جَآءَهُ ٱلۡأَعۡمَىٰ"
    },
    {
      "number": 3,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "وَمَا يُدۡرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ"
    },
    {
      "number": 4,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "أَوۡ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكۡرَىٰٓ"
    },
    {
      "number": 5,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "أَمَّا مَنِ ٱسۡتَغۡنَىٰ"
    },
    {
      "number": 6,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "فَأَنتَ لَهُۥ تَصَدَّىٰ"
    },
    {
      "number": 7,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "وَمَا عَلَيۡكَ أَلَّا يَزَّكَّىٰ"
    },
    {
      "number": 8,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "وَأَمَّا مَن جَآءَكَ يَسۡعَىٰ"
    },
    {
      "number": 9,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "وَهُوَ يَخۡشَىٰ"
    },
    {
      "number": 10,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "فَأَنتَ عَنۡهُ تَلَهَّىٰ"
    },
    {
      "number": 11,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "كَلَّآ إِنَّهَا تَذۡكِرَةٞ"
    },
    {
      "number": 12,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "فَمَن شَآءَ ذَكَرَهُۥ"
    },
    {
      "number": 13,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "فِي صُحُفٖ مُّكَرَّمَةٖ"
    },
    {
      "number": 14,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "مَّرۡفُوعَةٖ مُّطَهَّرَةِۭ"
    },
    {
      "number": 15,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "بِأَيۡدِي سَفَرَةٖ"
    },
    {
      "number": 16,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "كِرَامِۭ بَرَرَةٖ"
    },
    {
      "number": 17,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "قُتِلَ ٱلۡإِنسَٰنُ مَآ أَكۡفَرَهُۥ"
    },
    {
      "number": 18,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "مِنۡ أَيِّ شَيۡءٍ خَلَقَهُۥ"
    },
    {
      "number": 19,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "مِن نُّطۡفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ"
    },
    {
      "number": 20,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ"
    },
    {
      "number": 21,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "ثُمَّ أَمَاتَهُۥ فَأَقۡبَرَهُۥ"
    },
    {
      "number": 22,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ"
    },
    {
      "number": 23,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "كَلَّا لَمَّا يَقۡضِ مَآ أَمَرَهُۥ"
    },
    {
      "number": 24,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ إِلَىٰ طَعَامِهِۦٓ"
    },
    {
      "number": 25,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "أَنَّا صَبَبۡنَا ٱلۡمَآءَ صَبّٗا"
    },
    {
      "number": 26,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "ثُمَّ شَقَقۡنَا ٱلۡأَرۡضَ شَقّٗا"
    },
    {
      "number": 27,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "فَأَنۢبَتۡنَا فِيهَا حَبّٗا"
    },
    {
      "number": 28,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "وَعِنَبٗا وَقَضۡبٗا"
    },
    {
      "number": 29,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "وَزَيۡتُونٗا وَنَخۡلٗا"
    },
    {
      "number": 30,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "وَحَدَآئِقَ غُلۡبٗا"
    },
    {
      "number": 31,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "وَفَٰكِهَةٗ وَأَبّٗا"
    },
    {
      "number": 32,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "مَّتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ"
    },
    {
      "number": 33,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ"
    },
    {
      "number": 34,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "يَوۡمَ يَفِرُّ ٱلۡمَرۡءُ مِنۡ أَخِيهِ"
    },
    {
      "number": 35,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "وَأُمِّهِۦ وَأَبِيهِ"
    },
    {
      "number": 36,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "وَصَٰحِبَتِهِۦ وَبَنِيهِ"
    },
    {
      "number": 37,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "لِكُلِّ ٱمۡرِيٕٖ مِّنۡهُمۡ يَوۡمَئِذٖ شَأۡنٞ يُغۡنِيهِ"
    },
    {
      "number": 38,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "وُجُوهٞ يَوۡمَئِذٖ مُّسۡفِرَةٞ"
    },
    {
      "number": 39,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "ضَاحِكَةٞ مُّسۡتَبۡشِرَةٞ"
    },
    {
      "number": 40,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "وَوُجُوهٞ يَوۡمَئِذٍ عَلَيۡهَا غَبَرَةٞ"
    },
    {
      "number": 41,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "تَرۡهَقُهَا قَتَرَةٌ"
    },
    {
      "number": 42,
      "juz": 30,
      "page": 585,
      "sajda": false,
      "ar": "أُوْلَـٰٓئِكَ هُمُ ٱلۡكَفَرَةُ ٱلۡفَجَرَةُ"
    }
  ]
}