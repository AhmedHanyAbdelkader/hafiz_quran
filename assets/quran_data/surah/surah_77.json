{
  "number": 77,
  "name": {
    "ar": "المرسلات"
  },
  "revelation_place": {
    "ar": "مكية"
  },
  "verses_count": 50,
  "words_count": 181,
  "letters_count": 815,
  "verses": [
    {
      "number": 1,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "وَٱلۡمُرۡسَلَٰتِ عُرۡفٗا"
    },
    {
      "number": 2,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "فَٱلۡعَٰصِفَٰتِ عَصۡفٗا"
    },
    {
      "number": 3,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "وَٱلنَّـٰشِرَٰتِ نَشۡرٗا"
    },
    {
      "number": 4,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "فَٱلۡفَٰرِقَٰتِ فَرۡقٗا"
    },
    {
      "number": 5,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "فَٱلۡمُلۡقِيَٰتِ ذِكۡرًا"
    },
    {
      "number": 6,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "عُذۡرًا أَوۡ نُذۡرًا"
    },
    {
      "number": 7,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "إِنَّمَا تُوعَدُونَ لَوَٰقِعٞ"
    },
    {
      "number": 8,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "فَإِذَا ٱلنُّجُومُ طُمِسَتۡ"
    },
    {
      "number": 9,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "وَإِذَا ٱلسَّمَآءُ فُرِجَتۡ"
    },
    {
      "number": 10,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "وَإِذَا ٱلۡجِبَالُ نُسِفَتۡ"
    },
    {
      "number": 11,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "وَإِذَا ٱلرُّسُلُ أُقِّتَتۡ"
    },
    {
      "number": 12,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "لِأَيِّ يَوۡمٍ أُجِّلَتۡ"
    },
    {
      "number": 13,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "لِيَوۡمِ ٱلۡفَصۡلِ"
    },
    {
      "number": 14,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "وَمَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلۡفَصۡلِ"
    },
    {
      "number": 15,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "number": 16,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "أَلَمۡ نُهۡلِكِ ٱلۡأَوَّلِينَ"
    },
    {
      "number": 17,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "ثُمَّ نُتۡبِعُهُمُ ٱلۡأٓخِرِينَ"
    },
    {
      "number": 18,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "كَذَٰلِكَ نَفۡعَلُ بِٱلۡمُجۡرِمِينَ"
    },
    {
      "number": 19,
      "juz": 29,
      "page": 580,
      "sajda": false,
      "ar": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "number": 20,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "أَلَمۡ نَخۡلُقكُّم مِّن مَّآءٖ مَّهِينٖ"
    },
    {
      "number": 21,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "فَجَعَلۡنَٰهُ فِي قَرَارٖ مَّكِينٍ"
    },
    {
      "number": 22,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "إِلَىٰ قَدَرٖ مَّعۡلُومٖ"
    },
    {
      "number": 23,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "فَقَدَرۡنَا فَنِعۡمَ ٱلۡقَٰدِرُونَ"
    },
    {
      "number": 24,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "number": 25,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ كِفَاتًا"
    },
    {
      "number": 26,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "أَحۡيَآءٗ وَأَمۡوَٰتٗا"
    },
    {
      "number": 27,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "وَجَعَلۡنَا فِيهَا رَوَٰسِيَ شَٰمِخَٰتٖ وَأَسۡقَيۡنَٰكُم مَّآءٗ فُرَاتٗا"
    },
    {
      "number": 28,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "number": 29,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "ٱنطَلِقُوٓاْ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ"
    },
    {
      "number": 30,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "ٱنطَلِقُوٓاْ إِلَىٰ ظِلّٖ ذِي ثَلَٰثِ شُعَبٖ"
    },
    {
      "number": 31,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "لَّا ظَلِيلٖ وَلَا يُغۡنِي مِنَ ٱللَّهَبِ"
    },
    {
      "number": 32,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "إِنَّهَا تَرۡمِي بِشَرَرٖ كَٱلۡقَصۡرِ"
    },
    {
      "number": 33,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "كَأَنَّهُۥ جِمَٰلَتٞ صُفۡرٞ"
    },
    {
      "number": 34,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "number": 35,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "هَٰذَا يَوۡمُ لَا يَنطِقُونَ"
    },
    {
      "number": 36,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "وَلَا يُؤۡذَنُ لَهُمۡ فَيَعۡتَذِرُونَ"
    },
    {
      "number": 37,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "number": 38,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "هَٰذَا يَوۡمُ ٱلۡفَصۡلِۖ جَمَعۡنَٰكُمۡ وَٱلۡأَوَّلِينَ"
    },
    {
      "number": 39,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "فَإِن كَانَ لَكُمۡ كَيۡدٞ فَكِيدُونِ"
    },
    {
      "number": 40,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "number": 41,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "إِنَّ ٱلۡمُتَّقِينَ فِي ظِلَٰلٖ وَعُيُونٖ"
    },
    {
      "number": 42,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "وَفَوَٰكِهَ مِمَّا يَشۡتَهُونَ"
    },
    {
      "number": 43,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "كُلُواْ وَٱشۡرَبُواْ هَنِيٓـَٔۢا بِمَا كُنتُمۡ تَعۡمَلُونَ"
    },
    {
      "number": 44,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "إِنَّا كَذَٰلِكَ نَجۡزِي ٱلۡمُحۡسِنِينَ"
    },
    {
      "number": 45,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "number": 46,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "كُلُواْ وَتَمَتَّعُواْ قَلِيلًا إِنَّكُم مُّجۡرِمُونَ"
    },
    {
      "number": 47,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "number": 48,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "وَإِذَا قِيلَ لَهُمُ ٱرۡكَعُواْ لَا يَرۡكَعُونَ"
    },
    {
      "number": 49,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "number": 50,
      "juz": 29,
      "page": 581,
      "sajda": false,
      "ar": "فَبِأَيِّ حَدِيثِۭ بَعۡدَهُۥ يُؤۡمِنُونَ"
    }
  ]
}